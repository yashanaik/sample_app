<%= form_for(@city) do |f| %>
  <% if @city.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@city.errors.count, "error") %> prohibited this city from being saved:</h2>

      <ul>
      <% @city.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :cityname %><br>
    <%= f.collection_select :cityname, City.find(:all), :id, :cityname, :prompt => "Please select" %>
  </div>
  <div class="field">
    <%= f.label :statename %><br>
    <%= f.text_field :statename %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
